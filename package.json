{
  "name": "mapmoize",
  "version": "0.0.1",
  "description": "Memoize decorator for TypeScript that uses WeakMap",
  "keywords": [
    "typescript",
    "memoize",
    "memoization",
    "decorator",
    "weakmap",
    "weak-map",
    "lru",
    "lru-map"
  ],
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "test": "./node_modules/.bin/tsm ./node_modules/uvu/bin.js . src/__tests__/*.test.ts",
    "test:watch": "./node_modules/.bin/watchlist src -- pnpm run test",
    "benchmark": "pnpm run build && node ./dist/__tests__/index.bench.js",
    "prepublishOnly": "rm -rf ./dist && pnpm run build"
  },
  "author": "Sergey Ukustov <sergey@ukstv.me>",
  "license": "(MIT OR Apache-2.0)",
  "repository": "ukstv/mapmoize",
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=18.0.0"
  },
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "!dist/__tests__/"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.1",
    "@types/node": "^17.0.44",
    "benchmark": "^2.1.4",
    "prettier": "^2.7.0",
    "snoop": "^1.0.4",
    "tsm": "^2.2.1",
    "typescript": "^4.7.3",
    "typescript-memoize": "^1.1.0",
    "uvu": "^0.5.3",
    "watchlist": "^0.3.1"
  }
}
