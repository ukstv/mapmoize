{
  "name": "mapmoize",
  "version": "1.0.2",
  "description": "Memoize decorator for TypeScript that uses WeakMap",
  "keywords": [
    "typescript",
    "memoize",
    "memoization",
    "decorator",
    "weakmap",
    "weak-map",
    "lru",
    "lru-map"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "tsm ./node_modules/uvu/bin.js . src/__tests__/*.test.ts",
    "test:watch": "watchlist src -- pnpm run test",
    "size-limit": "size-limit",
    "prepublishOnly": "rm -rf ./dist && pnpm run build"
  },
  "author": "Sergey Ukustov <sergey@ukstv.me>",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ukstv/mapmoize.git"
  },
  "engines": {
    "node": "^14.0.0 || ^16.0.0 || >=18.0.0"
  },
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/benchmark": "^2.1.2",
    "@types/node": "^17.0.45",
    "benchmark": "^2.1.4",
    "prettier": "^2.7.1",
    "size-limit": "^8.1.0",
    "snoop": "^1.0.4",
    "tsm": "^2.2.2",
    "typescript": "^4.8.4",
    "typescript-memoize": "^1.1.1",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  }
}
